{
  "$schema": "node_modules/@tauri-apps/cli/schema.json",
  "build": {
    "devPath": "http://localhost:4200",
    "distDir": "../client/dist"
  },
  "package": {
    "productName": "tauri",
    "version": "0.0.0"
  },
  "plugins": {
    "cli": {
      "description": "Command example `sub1 sub2`",
      "subcommands": {
        "sub1": {
          "subcommands": {
            "sub2": {}
          }
        }
      }
    },
    "shell": {
      "open": true
    }
  },
  "tauri": {
    "bundle": {
      "active": true,
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "glazer.zebar",
      "resources": [
        "resources/*"
      ],
      "targets": "all"
    },
    "macOSPrivateApi": true,
    "security": {
      "csp": null
    },
    "windows": [
      {
        "decorations": false,
        "fullscreen": false,
        "height": 500,
        "resizable": false,
        "title": "Zebar",
        "transparent": false,
        "width": 500
      }
    ]
  }
}